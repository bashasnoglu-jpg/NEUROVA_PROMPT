{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["version", "packs"],
  "properties": {
    "version": {
      "type": "string"
    },
    "packs": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "file"],
        "properties": {
          "id": { "type": "string" },
          "file": { "type": "string" }
        }
      }
    }
  },
  "additionalProperties": true
}